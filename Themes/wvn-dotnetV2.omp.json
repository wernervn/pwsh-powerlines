{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "dotnet",
          "style": "diamond",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#ffff00",
          "properties": {
            "display_mode": "always",
            "prefix": " v"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#3A86FF",
          "background": "#242424",
          "properties" : {
            "prefix": " \uE5FF ",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#49eb34",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#99E6FF{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#F50505{{ end }}",
            "{{ if gt .Ahead 0 }}#FFFF00{{ end }}",
            "{{ if gt .Behind 0 }}#F505C9{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}",
            "prefix": ""
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "diamond",
          "leading_diamond": "\ue0c2",
          "trailing_diamond": "\ue0c0",
          "foreground": "#ffffff",
          "background": "#ff4040",
          "properties": {
            "display_exit_code": false,
            "always_enabled": false,
            "prefix": " \ue70f"
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "houston"
          }
        }
      ]
    }
  ],
  "final_space": true
}
